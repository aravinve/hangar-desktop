{
  "name": "hangar",
  "version": "1.0.0",
  "description": "All Apps In Your Hangar",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.3.0",
    "autoprefixer": "^10.0.4",
    "concurrently": "^5.3.0",
    "convert-units": "^2.3.4",
    "cross-env": "^7.0.3",
    "dataurl": "^0.1.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-store": "^6.0.1",
    "electron-updater": "^4.3.5",
    "firebase": "^8.2.0",
    "google-map-react": "^2.1.9",
    "mime-types": "^2.1.28",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32",
    "music-metadata": "^7.6.4",
    "postcss": "^8.1.14",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-quill": "^1.3.5",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "tailwind": "^4.0.0",
    "uuid": "^8.3.1",
    "wait-on": "^5.2.0"
  },
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "build": "npm run react-build && npm run electron-build",
    "build:tailwind": "tailwindcss build src/tailwind.css -o src/index.css",
    "electron-dev": "concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder --win -c.extraMetadata.main=build/electron.js",
    "preelectron-pack": "npm run build",
    "gh-publish": "electron-builder --win -p onTag"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "main": "public/electron.js",
  "author": "Aravind Venkatesan",
  "repository": "git@github.com:aravinve/hangar.git",
  "build": {
    "appId": "com.example.electron-cra",
    "win": {
      "icon": "build/icon.png"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "homepage": "./",
  "devDependencies": {
    "electron": "^15.5.5",
    "electron-builder": "^22.9.1"
  }
}
